{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 13,
    "sourceType": "module"
  },
  "rules": {
    "no-console": 0,
    "linebreak-style": 0,
    "no-tabs": 0,
    "no-underscore-dangle": 0,
    "eslint-disable  ": "indent"
  }
}